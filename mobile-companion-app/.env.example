# Environment Configuration for Dementia Care Monitor

# Build Configuration
BRAND_CONFIG=wellfit
EAS_PROJECT_ID=your-eas-project-id
EXPO_OWNER=wellfitcommunity

# Google Maps API (Required for map functionality)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Branding (Optional - overrides defaults)
BRAND_COMPANY_NAME=WellFit Community
BRAND_APP_NAME=Dementia Care Monitor
BRAND_TAGLINE=HIPAA-Compliant Patient Safety Monitoring
BRAND_PRIMARY_COLOR=#2196F3
BRAND_SECONDARY_COLOR=#4CAF50
BRAND_ACCENT_COLOR=#FF9800
BRAND_EMERGENCY_COLOR=#F44336

# Contact Information
BRAND_SUPPORT_EMAIL=support@wellfitcommunity.org
BRAND_EMERGENCY_EMAIL=emergency@wellfitcommunity.org
BRAND_PRIVACY_EMAIL=privacy@wellfitcommunity.org
BRAND_PHONE=1-800-WELLFIT

# Legal
BRAND_WEBSITE=https://wellfitcommunity.org
BRAND_PRIVACY_URL=https://wellfitcommunity.org/privacy
BRAND_TERMS_URL=https://wellfitcommunity.org/terms

# App Store
BRAND_PACKAGE_NAME=com.wellfitcommunity.dementiacare
BRAND_KEYWORDS=dementia care, patient monitoring, geofencing, health tracking

# Feature Toggles
ENABLE_GEOFENCING=true
ENABLE_HEALTH_MONITORING=true
ENABLE_EMERGENCY_ALERTS=true
ENABLE_LOCATION_HISTORY=true
ENABLE_DATA_EXPORT=true

# iOS App Store Connect (for submission)
APPLE_ID=your-apple-id@email.com
ASC_APP_ID=your-app-store-connect-app-id
APPLE_TEAM_ID=your-apple-team-id

# Android Play Console (for submission)
ANDROID_SERVICE_ACCOUNT_JSON=./android-service-account.json