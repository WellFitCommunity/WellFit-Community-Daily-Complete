# White Label Configuration Template
# Copy this file to .env and customize for your brand

# BRAND CONFIGURATION
# Set to "wellfit" for WellFit Community, or "custom" for white label
BRAND_CONFIG=wellfit

# COMPANY INFORMATION
BRAND_COMPANY_NAME="Your Healthcare Company"
BRAND_APP_NAME="Care Monitor"
BRAND_TAGLINE="Professional Patient Safety Monitoring"

# VISUAL BRANDING (HEX COLOR CODES)
BRAND_PRIMARY_COLOR="#2196F3"
BRAND_SECONDARY_COLOR="#4CAF50"
BRAND_ACCENT_COLOR="#FF9800"
BRAND_BACKGROUND_COLOR="#FFFFFF"
BRAND_TEXT_COLOR="#333333"
BRAND_EMERGENCY_COLOR="#F44336"

# CONTACT INFORMATION
BRAND_SUPPORT_EMAIL="support@yourcompany.com"
BRAND_EMERGENCY_EMAIL="emergency@yourcompany.com"
BRAND_PRIVACY_EMAIL="privacy@yourcompany.com"
BRAND_PHONE="1-800-YOURCARE"
BRAND_EMERGENCY_PHONE="1-800-YOURCARE ext. 911"

# ADDRESS
BRAND_ADDRESS_STREET="123 Healthcare Drive"
BRAND_ADDRESS_CITY="Medical City"
BRAND_ADDRESS_STATE="MC"
BRAND_ADDRESS_ZIP="12345"

# LEGAL INFORMATION
BRAND_LEGAL_ENTITY="Your Healthcare Company LLC"
BRAND_WEBSITE="https://yourcompany.com"
BRAND_PRIVACY_URL="https://yourcompany.com/privacy"
BRAND_TERMS_URL="https://yourcompany.com/terms"

# APP STORE CONFIGURATION
BRAND_PACKAGE_NAME="com.yourcompany.caremonitor"
BRAND_KEYWORDS="health monitoring, patient care, medical alert"
BRAND_SHORT_DESC="Professional patient monitoring and safety alerts"

# FEATURE TOGGLES (set to "false" to disable)
ENABLE_GEOFENCING="true"
ENABLE_HEALTH_MONITORING="true"
ENABLE_EMERGENCY_ALERTS="true"
ENABLE_LOCATION_HISTORY="true"
ENABLE_DATA_EXPORT="true"

# EXTERNAL SERVICES
GOOGLE_MAPS_API_KEY="your_google_maps_api_key"
EAS_PROJECT_ID="your_eas_project_id"