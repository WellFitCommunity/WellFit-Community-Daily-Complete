  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "start:cs": "DISABLE_ESLINT_PLUGIN=true PORT=3100 BROWSER=none WDS_SOCKET_HOST=0.0.0.0 WDS_SOCKET_PORT=3100 CHOKIDAR_USEPOLLING=true react-scripts start",
    "start:cs-debug": "DISABLE_ESLINT_PLUGIN=true CI=true CHOKIDAR_USEPOLLING=true PORT=3100 BROWSER=none WDS_SOCKET_HOST=0.0.0.0 WDS_SOCKET_PORT=3100 react-scripts start -- --progress",
    "build": "DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts build",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test --testPathIgnorePatterns=integration",
    "test:unit": "DISABLE_ESLINT_PLUGIN=true react-scripts test --testPathIgnorePatterns=integration --watchAll=false",
    "test:integration": "DISABLE_ESLINT_PLUGIN=true react-scripts test --testPathPattern=integration --watchAll=false",
    "test:security": "DISABLE_ESLINT_PLUGIN=true react-scripts test --testPathPattern=security --watchAll=false",
    "test:all": "DISABLE_ESLINT_PLUGIN=true react-scripts test --watchAll=false",
    "test:coverage": "DISABLE_ESLINT_PLUGIN=true react-scripts test --coverage --watchAll=false --testPathIgnorePatterns=integration",
    "db:migrate": "./scripts/migrate.sh local",
    "db:migrate:staging": "./scripts/migrate.sh staging",
    "db:migrate:production": "./scripts/migrate.sh production",
    "db:new-migration": "./scripts/create-migration.sh",
    "test:security:coverage": "DISABLE_ESLINT_PLUGIN=true react-scripts test --coverage --testPathPattern=security --watchAll=false",
    "test:ci": "npm run test:unit && npm run test:integration",
    "typecheck": "tsc --noEmit",
    "eject": "react-scripts eject",
    "serve:register": "npx supabase functions serve --env-file ./supabase/.env.local --no-verify-jwt",
    "deploy:register": "npx supabase functions deploy register",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:security": "eslint --config .eslintrc.security.js --ext .js,.ts,.tsx src/ supabase/",
    "security:check": "./scripts/security-check.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "sb:version": "npx -y supabase@latest --version",
    "sb:migrations": "npx -y supabase@latest migration list",
    "sb:start": "npx -y supabase@latest start -x edge-runtime",
    "sb:diff": "npx -y supabase@latest db diff -f rls-hardening --debug",
    "sb:stop": "npx -y supabase@latest stop",
    "serve:3100": "npx serve -s build -l 3100",
    "sbom:generate": "node scripts/generate-sbom.js",
    "deps:audit": "npm audit --audit-level=moderate && npm outdated",
    "deps:update": "npm update && npm run sbom:generate",
    "disaster-recovery:drill:weekly": "./scripts/disaster-recovery/execute-weekly-drill.sh",
    "disaster-recovery:drill:monthly": "./scripts/disaster-recovery/execute-monthly-drill.sh",
    "pentest:daily": "./scripts/penetration-testing/daily-scan.sh",
    "pentest:weekly": "./scripts/penetration-testing/weekly-scan.sh",
    "compliance:report": "./scripts/compliance/generate-report.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
